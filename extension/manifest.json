{
"name": "[DEV] Progress Bar Timer",
	"version": "1.71",
	"manifest_version": 2,
	"description": "Make general purpose trackers: progress bars that count time, dates, and anything you want.",
	"icons": { "16": "images/icon16.png",
	             "48": "images/icon48.png",
	            "128": "images/icon128.png" },
	"app" : {
		"launch" : {
			"local_path" : "progress.html"
		}
	},
	"offline_enabled": true,
	"permissions" : [
		"storage", "notifications" 
	],
	"web_accessible_resources" : [
		"images/icon48.png"
	]
}
